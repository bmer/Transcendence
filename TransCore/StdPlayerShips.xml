						imageX="0" imageY="0" imageWidth="90" imageHeight="9"
						destX="23" destY="32" hpX="55" hpY="27"
						nameY="8" nameBreakWidth="360" nameDestX="12" nameDestY="0" />

				<ArmorSection name="starboard"
